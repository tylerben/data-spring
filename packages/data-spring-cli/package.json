{
  "name": "data-spring-cli",
  "description": "Data Spring command-line interface for generating fake datasets.",
  "version": "0.1.1",
  "main": "index.js",
  "author": "tylerben <ben@stae.co>",
  "license": "MIT",
  "scripts": {
    "data-spring": "ts-node ./src/cli.ts",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && tsc && chmod +x dist/cli.js",
    "prepare": "yarn build",
    "check-types": "tsc --noEmit",
    "check-formatting": "prettier --check '**/*.{js,json,css,md,scss,tsx,ts}'",
    "commit": "commit"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "data-spring": "./dist/cli.js"
  },
  "dependencies": {
    "data-spring": "^0.1.1",
    "yargonaut": "^1.1.4",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@types/node": "^14.11.5",
    "@types/yargs": "^15.0.7",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "gitHead": "488afa571b1007f2ca76b30e06f78699a29d01b2"
}
